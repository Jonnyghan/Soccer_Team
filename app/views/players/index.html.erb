<h1> Players!</h1>


<% @players.each do |player|%>
    <h2><a href='/players/<%=player.id%>'> <%= player.name %> </a></h2>
    <h4>Age: <%=player.age%></h4><br>
    <h4>Position: <%=player.position%></h4><br><br>
<%end%>
<%if session[:user_id] == 13 %>
<a href="/players/new"> Create A New Player</a>  
<%end%>

